    scale 3 0.1 3;
    move 6 0 9;
    rename "Cell48";
    polyCube;
    scale 3 0.1 3;
    move 9 0 9;
    rename "Cell49";
    for ($x = 0; $x < 49; $x++)
{
    int $number = `rand 1 4`;
    if ($number == 1) {
    string $head[] = `polySphere`;
    select $head;
    move 0 5 0;
    rename "Head";
    string $torso[] = `polyCylinder`;
    select $torso;
    move 0 3 0;
    scale 0.25 1 0.25;
    rename "Torso";
    string $left_arm[] = `polyCylinder`;
    select $left_arm;
    move -0.75 2.5 0;
    rotate 0 0 -20;
    scale 0.25 1 0.25;
    rename "Left_Arm";
    string $right_arm[] = `polyCylinder`;
    select $right_arm;
    move 0.75 2.5 0;
    rotate 0 0 20;
    scale 0.25 1 0.25;
    rename "Right_Arm";
    string $left_leg[] = `polyCylinder`;
    select $left_leg;
    move -0.5 1 0;
    rotate 0 0 -20;
    scale 0.25 1 0.25;
    rename "Left_Leg";
    string $right_leg[] = `polyCylinder`;
    select $right_leg;
    move 0.5 1 0;
    rotate 0 0 20;
    scale 0.25 1 0.25;
    rename "Right_Leg";
    string $shader = `shadingNode -asShader lambert`;
    string $shadingGrp = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader + ".outColor") ($shadingGrp + ".surfaceShader");
    setAttr ($shader + ".color") 0.95 0.95 0.75;
    select Left_Arm Right_Arm Left_Leg Right_Leg Torso Head;
    hyperShade -assign $shader;
    parent;
    select Head;
    move 0 1 0;
    scale 0.19 0.19 0.19;
    select Left_Arm;
    rename "L_A";
    select Right_Arm;
    rename "R_A";
    select Left_Leg;
    rename "L_L";
    select Right_Leg;
    rename "R_L";
    select Torso;
    rename "T";
    select Head;
    rename "H";
    }
    else if ($number == 2) {
    string $base[] = `polyCube`;
    select $base;
    scale 10 15 10;
    move 0 7.5 0;
    rename "Base";
    string $window1[] = `polyCube`;
    select $window1;
    scale 5 5 0.5;
    move 0 10 5.3;
    rename "Window1";
    string $window2[] = `polyCube`;
    select $window2;
    scale 5 5 0.5;
    move -5.3 10 0;
    rotate 0 90 0;
    rename "Window2";
    string $window3[] = `polyCube`;
    select $window3;
    scale 5 5 0.5;
    move 0 10 -5.3;
    rename "Window3";
    string $window4[] = `polyCube`;
    select $window4;
    scale 5 5 0.5;
    move 5.3 10 0;
    rotate 0 90 0;
    rename "Window4";
    string $shader = `shadingNode -asShader lambert`;
    string $shadingGrp = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader + ".outColor") ($shadingGrp + ".surfaceShader");
    setAttr ($shader + ".color") 1 0.7 0.5;
    string $shader2 = `shadingNode -asShader lambert`;
    string $shadingGrp2 = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader2 + ".outColor") ($shadingGrp2 + ".surfaceShader");
    setAttr ($shader2 + ".color") 0 1 1;
    select Base;
    hyperShade -assign $shader;
    select Window1 Window2 Window3 Window4;
    hyperShade -assign $shader2;
    select Window1 Window2 Window3 Window4 Base;
    parent;
    select Base;
    move 0 1.1 0;
    scale 2.5 4 2.5;
    makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
    select Window1;
    rename "W_1";
    select Window2;
    rename "W_2";
    select Window3;
    rename "W_3";
    select Window4;
    rename "W_4";
    select Base;
    rename "House";
    }
    else if ($number == 3) {
    string $top[] = `polySphere`;
    select $top;
    scale 3 3 3;
    move 0 10 0;
    rename "Top";
    string $bottom[] = `polyCylinder`;
    select $bottom;
    scale 1 4 1;
    move 0 4 0;
    rename "Bottom";
    string $shader = `shadingNode -asShader lambert`;
    string $shadingGrp = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader + ".outColor") ($shadingGrp + ".surfaceShader");
    setAttr ($shader + ".color") 0 1 0;
    string $shader2 = `shadingNode -asShader lambert`;
    string $shadingGrp2 = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader2 + ".outColor") ($shadingGrp2 + ".surfaceShader");
    setAttr ($shader2 + ".color") 0.1 0 0;
    select Top;
    hyperShade -assign $shader;
    select Bottom;
    hyperShade -assign $shader2;
    select Bottom Top;
    parent;
    select Top;
    move 0 2.4 0;
    scale 1 1 1;
    makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
    select Bottom;
    rename "Trunk";
    select Top;
    rename "Leaves";
    }
    if ($x == 0) {
        move -9 1 -9;
    }
    else if ($x == 1) {
        delete;
    }
    else if ($x == 2) {
        move -3 1 -9;
    }
    else if ($x == 3) {
        delete;
    }
    else if ($x == 4) {
