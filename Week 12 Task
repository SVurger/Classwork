    }
    else if ($x == 9) {
        move -3 1 -6;
    }
    else if ($x == 10) {
        delete;
    }
    else if ($x == 11) {
        move 3 1 -6;
    }
    else if ($x == 12) {
        delete;
    }
    else if ($x == 13) {
        move 9 1 -6;
    }
    else if ($x == 14) {
        move -9 1 -3;
    }
    else if ($x == 15) {
        delete;
    }
    else if ($x == 16) {
        move -3 1 -3;
    }
    else if ($x == 17) {
        delete;
    }
    else if ($x == 18) {
        move 3 1 -3;
    }
    else if ($x == 19) {
        delete;
    }
    else if ($x == 20) {
        move 9 1 -3;
    }
    else if ($x == 21) {
        move -9 1 0;
    }
    else if ($x == 22) {
        delete;
    }
    else if ($x == 23) {
        move -3 1 0;
    }
    else if ($x == 24) {
