//Define a procedure for "createTree".
global proc createTree() {
    string $top[] = `polySphere`;
    select $top;
    scale 3 3 3;
    move 0 10 0;
    rename "Top";
    string $bottom[] = `polyCylinder`;
    select $bottom;
    scale 1 4 1;
    move 0 4 0;
    rename "Bottom";
    string $shader = `shadingNode -asShader lambert`;
    string $shadingGrp = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader + ".outColor") ($shadingGrp + ".surfaceShader");
    string $inputText = `textField -query -text textField3`;
    if ($inputText == "Orange") {
        setAttr ($shader + ".color") 1 0.3 0;
    }
    else if ($inputText == "Yellow") {
        setAttr ($shader + ".color") 1 0.9 0;
    }
    else if ($inputText == "Pink") {
        setAttr ($shader + ".color") 1 0.7 0.8;
    }
    else {
        setAttr ($shader + ".color") 0 1 0;
    }
    string $shader2 = `shadingNode -asShader lambert`;
    string $shadingGrp2 = `sets -renderable true -noSurfaceShader true -empty`;
    connectAttr ($shader2 + ".outColor") ($shadingGrp2 + ".surfaceShader");
    setAttr ($shader2 + ".color") 0.1 0 0;
    select Top;
    hyperShade -assign $shader;
    select Bottom;
    hyperShade -assign $shader2;
    select Bottom Top;
    parent;
    select Top;
    move 0 3.4 0;
    scale 1 1 1;
    makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
    select Bottom;
    rename "Trunk";
    select Top;
    rename "Leaves";
    }

//Build the window
window -title "City Generator" -widthHeight 200 100 cityGenerator;
columnLayout -adjustableColumn true -rowSpacing 20;
    text -label "Please select an environment below!";
    separator -height 10 -style "none";
    text -label "Create a small street!";
    button -label "Create Street" -command "createStreet()" -bgc 0 255 0; //Green button for a street.
    separator -height 10 -style "none";
    text -label "Create a medium town!";
    button -label "Create Town" -command "createTown()" -bgc 255 165 0; //Yellow button for a town.
    separator -height 10 -style "none";
    text -label "Create a large city!";
    button -label "Create City" -command "createCity()" -bgc 255 0 0; //Red button for a city.
    separator -height 10 -style "none";
    text -label "Create individual assets!";
    button -label "Create Road" -command "createRoad()" -bgc 0.530 0.810 0.920; //Blue buttons for individual assets.
    button -label "Create Stickman" -command "createStickman()" -bgc 0.530 0.810 0.920;
    text -label "Stickman colour (Red/Green/Blue):";
    textField -width 250 textField1;
    button -label "Create House" -command "createHouse()" -bgc 0.530 0.810 0.920;
    text -label "House window number (1/2/3):";
    textField -width 250 textField2;
    button -label "Create Tree" -command "createTree()" -bgc 0.530 0.810 0.920;
    text -label "Tree colour (Orange/Yellow/Pink):";
    textField -width 250 textField3;
